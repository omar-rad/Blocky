        <activity
            android:name="com.example.blocky.AnalogClock"
            android:exported="false"
            android:label="@string/analog_clock" />
        <activity
            android:name="com.example.blocky.Home"
            android:exported="false" />
        <activity
            android:name="com.example.blocky.MainActivity"
            android:exported="true"
            android:theme="@style/Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>